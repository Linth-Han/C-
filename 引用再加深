#include"iostream"
using namespace std;
class A{
public:
    int i;
    A():i(0){}
};
A f()
{
    return a;
}
int g()
{
    int i=10;
    return i;
}
int main()
{
    A b;
    b.i=20;
    f()=b;//语法无效 f()返回的是一个临时对象，无法赋值
    A temp=f();
    cout<<temp.i<<endl;//最后得到0
    system("pause");
    return 0;
}
//修改如下
#include"iostream"
using namespace std;
class A{
public:
    int i;
    A():i(0){}
};
A a;
A& f()
{
    return a;
}
int g()
{
    int i=10;
    return i;
}
int main()
{
    A b;
    b.i=20;
    f()=b;//此时语法有效，我们能够得到f（）=b能成功地将a中的i变为20
    A temp=f();
    cout<<temp.i<<endl;//最后我们得到20
    system("pause");
    return 0;
}
